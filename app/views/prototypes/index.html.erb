<main class="main">
  <div class="inner">
    <% if user_signed_in? %>
      <div class="greeting">
        <p><%= "こんにちは、" %></p>
        <%= link_to "#{current_user.user_name}さん", user_path(current_user), class: :greeting__link %>
      </div>
    <% end %>
    <%# // ログインしているときは上記を表示する %>
    <div class="card__wrapper">
      <%= render partial: 'prototype', collection: @prototypes %>
    </div>
  </div>
</main>
